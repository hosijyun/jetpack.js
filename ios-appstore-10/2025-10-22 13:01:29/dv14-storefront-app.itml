            color: @primaryTextColor@;
        }
        .divider-no-border {
            border-color: rgba(0,0,0,0);
        }
        .one-column {
            column-count: 1;
        }

        .carousel {
            padding: 0 5 15 0;
        }

        .secondary-color {
            color: @primaryTextColor60@;
        }
        .link-color {
            color: @linkColor@;
        }
        .action-color {
            color: @actionColor@;
        }
        .button-color {
            color: @actionColor@;
        }
        .bg-color {
            background-color: @backgroundColor@;
        }

        .key-color {
            color: @keyColor@;
        }

        .filter-bar-color {
            color: @filterBarColor@;
        }

        .selected {
            color: @keyColor@;
        }
        <!-- align/position styles -->
        .align-left {
            itml-align: left;
        }
        .align-right {
            itml-align: right;
        }

        .align-center {
            itml-align: center;
        }

        .align-top {
            itml-align: top;
        }
        .align-bottom {
            itml-align: bottom;
        }

        .text-left {
            text-align: left;
        }
        .text-right {
            text-align: right;
        }
        .text-center {
            text-align: center;
        }
        .text-top {
            text-align: top;
        }
        .text-bottom {
            text-align: bottom;
        }

        .position-top {
            itml-position: top;
        }
        .position-right {
            itml-position: right;
        }
        .position-center {
            itml-position: center;
        }
        .top-right {
            itml-position: top-right;
